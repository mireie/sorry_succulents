<%= render "layouts/errors", :object => @product %>
<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cost %>
  <%= f.number_field :cost %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <%= f.submit %>
<%end%>
